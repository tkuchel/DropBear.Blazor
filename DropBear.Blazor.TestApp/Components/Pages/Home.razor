@page "/"
@using DropBear.Blazor.Components.Alerts
@using DropBear.Blazor.Enums
@using DropBear.Blazor.Interfaces
@rendermode InteractiveServer
@inject ISnackbarNotificationService SnackbarService
<DropBearSnackbarNotificationContainer/>

@code{

    protected override void OnAfterRender(bool firstRender)
    {
        if (firstRender)
        {
            SnackbarService.ShowAsync("Welcome to the MochaDBV3.Blazor application!", SnackbarType.Success, duration: 5000);
        }
    }


}
